   "source": [
    "class Friend:\n",
    "    def __init__(self, name, age):\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "\n",
    "    def greet(self):\n",
    "        print(f\"Hey, I'm {self.name}!\")\n",
    "\n",
    "    def show_age(self):\n",
    "        print(f\"I am {self.age} years old.\")\n",
    "\n",
    "\n",
    "f = Friend(\"Brian\", 21)\n",
    "\n",
    "f.greet()\n",
    "f.show_age()\n"
   ]
